global proc tjh_batch_attr_set_Main(){	string $srcPath = `whatIs "tjh_batch_attr_set_Main"`;	int $size = size($srcPath) ;	$srcPath = `substring $srcPath 25 $size`;	$srcPath = dirname( $srcPath );	string $mayaVer=`about -v`;	string $ver[];	int $number=`tokenize $mayaVer "." $ver`;	string $plug_ins="linkDll_"+$ver[0]+".mll";	if(!` pluginInfo -query -l $plug_ins `)loadPlugin -qt ($srcPath+"/lib/"+$plug_ins);	string $dstScriptPath = `internalVar -userAppDir` + "scripts/"; sysFile -copy ($dstScriptPath+"tjh_batch_attr_set.dll") ($srcPath+"/lib/"+"tjh_batch_attr_set.dll"); linkDll ($dstScriptPath+"tjh_batch_attr_set.dll");tjh_batch_attr_set;}tjh_batch_attr_set_Main;